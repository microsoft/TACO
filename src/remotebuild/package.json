{
    "name": "remotebuild",
    "description": "Front-end server that serves modules that implement remote build functionality, such as taco-remote.",
    "version": "2.1.1-dev",
    "author": {
        "name": "Microsoft Corporation",
        "email": "vscordovatools-admin@microsoft.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Microsoft/TACO"
    },
    "bugs": {
        "url" : "https://github.com/Microsoft/TACO/issues", 
        "email" : "vscordovatools-admin@microsoft.com"
    },
    "homepage": "http://taco.tools",
    "keywords": [
        "cordova",
        "osx ",
        "remote build"
    ],
    "main": "./lib/server.js",
    "bin": {
        "remotebuild": "./bin/remotebuild"
    },
    "preferGlobal": true,
    "dependencies": {
        "express": "4.12.2",
        "morgan": "1.5.1",
        "errorhandler": "1.3.4",
        "helmet": "~0.12.0",
        "nconf": "0.6.9",
        "q": "1.0.1",
        "rimraf": "2.2.6",
        "semver": "^4.3.6",
        "taco-utils": "file:../taco-utils"
    },
    "optionalDependencies": { 
        "taco-remote": "file:../taco-remote"
    },
    "devDependencies": {
        "mocha": "2.0.1",
        "istanbul": "^0.4.0",
        "mkdirp": "0.3.5",
        "should": "4.3.0",
        "request": "2.36.0",
        "colors": "^1.0.3",
        "taco-tests-utils": "file:../taco-tests-utils"
    },
    "scripts": {
        "test": "mocha",
        "coverage": "istanbul cover --report json node_modules/mocha/bin/_mocha -- --reporter spec"
    },
    "directories": {
        "lib": "lib",
        "doc": ".",
        "test": "test",
        "example": "examples"
    },
    "readmeFilename": "README.md",
    "license": "MIT"
}
