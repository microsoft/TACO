{
    "name": "taco-remote",
    "description": "Agent to remotely build, run, and debug iOS apps created using Visual Studio Tools for Apache Cordova.",
    "version": "0.2.6",
    "author": {
        "name": "Microsoft Corporation",
        "email": "vscordovatools-admin@microsoft.com"
    },
    "homepage": "http://msdn.microsoft.com/en-us/vstudio/dn722381",
    "main": "./lib/server.js",
    "bin": {
        "taco-remote": "./bin/taco-remote"
    },

    "preferGlobal": true,
    "dependencies": {
        "express": "4.12.2",
        "morgan": "1.5.1",
        "errorhandler": "1.3.4",
        "request": "2.36.0",
        "fstream": "0.1.28",
        "nconf": "0.6.9",
        "q": "1.0.1",
        "rimraf": "2.2.6",
        "taco-utils": "file:../taco-utils"
    },
    "devDependencies": {
        "typescript": "1.3.0",
        "mocha": "2.0.1",
        "mkdirp": "0.3.5",
        "should": "4.3.0",
        "taco-cordova":  "file:../taco-cordova"
    },
    "bundledDependencies": [ "taco-utils" ],

    "scripts": {
        "test": "mocha"
    },
    "directories": {
        "lib": "lib",
        "doc": ".",
        "test": "test",
        "example": "examples"
    },
    "readmeFilename": "README.md"
}
